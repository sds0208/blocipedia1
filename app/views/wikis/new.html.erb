<h1 class="center-block">Create a New Wiki</h1>
<div class="container center-block">
    <%= form_for @wiki do |f| %>
        <div class="form-group">
            <%= f.label :title %>
            <%= f.text_field :title %>
        </div>
        <div class="form-group">
            <%= f.label :body %>
            <%= f.text_area :body %>
        </div>
        <%= f.submit "Save" %>
    <% end %>
</div>    